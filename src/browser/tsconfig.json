{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es2020", "dom", "dom.iterable"],
    "module": "none",
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../out/browser",
    "declaration": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "types": []
  },
  "include": ["./**/*.ts"],
  "exclude": []
}